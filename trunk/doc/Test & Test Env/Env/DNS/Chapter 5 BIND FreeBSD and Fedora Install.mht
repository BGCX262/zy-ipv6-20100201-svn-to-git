		v =3D v + document.body.scrollTop;=0A=
	}=0A=
	// are we still in area=0A=
	for(i =3D x+1;i > 0; i--)=0A=
	{=0A=
		if(clip(h,v) =3D=3D true)=0A=
		{=0A=
			return true;	// still OK=0A=
		}=0A=
		// out of one area=0A=
		unpop();=0A=
		x--;	// decrement index=0A=
	}=0A=
	return true;=0A=
}=0A=
function clip(h,v)=0A=
{=0A=
	if (h < rightpop[x] && h > rightnav[x])=0A=
	{=0A=
		// left=0A=
		if (v > topall[x] && v < lowpop[x])=0A=
		{=0A=
			return true;=0A=
		}=0A=
	}=0A=
	else=0A=
	{=0A=
		// alert("h=3D"+h+" rn=3D"+rightnav[x]);=0A=
	}=0A=
	return false;=0A=
}=0A=
function unpop()=0A=
{=0A=
	// out of area=0A=
	pop1[x].style.visibility =3D "hidden";=0A=
	if(x) return;	// return if index non-zero=0A=
	cn =3D menu.className=0A=
	if((pos =3D cn.lastIndexOf("-")) !=3D -1)=0A=
	{=0A=
		bcn =3D cn.substring(0,pos + 1);=0A=
		scn =3D cn.substring(pos +1);=0A=
		if(scn =3D=3D 'o')=0A=
		{=0A=
			menu.className =3D bcn + "f";=0A=
			if(way =3D=3D 'h') menus.className =3D bcn + "f";=0A=
		}=0A=
	}=0A=
}=0A=
function popclean(s, axis)=0A=
{=0A=
	way =3D axis;=0A=
// ensure that everything except 's' is invisible and 'off'=0A=
	for(i=3D0;i < 10; i++)=0A=
	{=0A=
		if(i !=3D s)=0A=
		{=0A=
			o1 =3D document.all["p" + i];=0A=
			if(o1)=0A=
			{=0A=
  			cn =3D o1.className=0A=
  			if((pos =3D cn.lastIndexOf("-")) !=3D -1)=0A=
  			{=0A=
  				bcn =3D cn.substring(0,pos + 1);=0A=
  				scn =3D cn.substring(pos +1);=0A=
  				if(scn =3D=3D 'o')=0A=
  				{=0A=
  					o1.className =3D bcn + "f";=0A=
  					if(way =3D=3D'h')=0A=
  					{=0A=
  						o3 =3D document.all["s" + i];=0A=
  					 	o3.className =3D bcn + "f";=0A=
  					}=0A=
  				}=0A=
  			}=0A=
  			o2 =3D document.all["h" + i];=0A=
  			o2.style.visibility =3D "hidden";=0A=
			}=0A=
		}=0A=
	}=0A=
}=0A=
=0A=
=0A=
//-->=0A=
